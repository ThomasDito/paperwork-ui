{
  "name": "paperwork-ui",
  "version": "1.0.0",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build:ts": "swc ./src -D ./package.json -d ./dist",
    "build:dts": "tsc -d --declarationMap --emitDeclarationOnly",
    "build:css": "postcss ./src/index.min.css --dir ./dist",
    "build": "npm run build:ts && npm run build:css && npm run build:dts",
    "eslint:check": "eslint ./src --ext js,jsx,ts,tsx --max-warnings=0",
    "prettier:check": "prettier ./src --check",
    "watch:ts": "swc -w ./src -D ./package.json -d ./dist",
    "watch:css": "postcss ./src/index.min.css -o ./dist/index.min.css --watch",
    "watch:dts": "tsc -w -d --declarationMap --emitDeclarationOnly"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-hover-card": "^1.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "formik": "^2.4.4",
    "lucide-react": "^0.274.0",
    "moment": "^2.29.4",
    "react": "^17 || ^18",
    "react-day-picker": "^8.8.1",
    "react-dom": "^17 || ^18",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.83",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "typescript": "^5.2.2"
  }
}
